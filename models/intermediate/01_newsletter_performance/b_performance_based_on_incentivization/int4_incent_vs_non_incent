with incent as (
    select * from {{ref('int2_incent')}}
),

non_incent as (
    select * from {{ref('int3_non_incent')}}
),

incent_vs_non_incent as (
    select
        Campaign_Date,
        Country,
        City,
        Incentivization,
        Delivered,
        Total_Opens,
        Unique_Opens,
        Unique_Open_Rate,
        Total_Clicks,
        Unique_Clicks,
        Total_CTOR
    from incent
    FULL OUTER JOIN non_incent using (Campaign_Date, Country, City, Incentivization)
)

select * from incent_vs_non_incent